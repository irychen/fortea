        "@types/jest": "^29.5.11",
        "@types/jsonwebtoken": "^9.0.5",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jsonwebtoken": "^9.0.2",
        "lint-staged": "^15.2.0",
        "prettier": "3.2.4",
        "rollup": "^4.9.6",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3"
